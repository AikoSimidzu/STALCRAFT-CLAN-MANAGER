<UserControl x:Class="StalcraftClanManager.Panels.SettingsPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StalcraftClanManager.Panels"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="930"
             Loaded="UserControl_Loaded">
    <Grid>
        <Label Content="WebHook" Foreground="White" FontSize="18" Margin="10,13,813,453"/>
        <TextBox x:Name="webHook" Background="#01FFFFFF" Foreground="White" BorderThickness="0,0,0,1" VerticalContentAlignment="Center" Height="20" Width="200" FontSize="15" Margin="133,20,597,460"/>

        <Label Content="WebHook для таблицы" Foreground="White" FontSize="18" Margin="10,61,709,405"/>
        <TextBox x:Name="webHookFT" Background="#01FFFFFF" Foreground="White" BorderThickness="0,0,0,1" VerticalContentAlignment="Center" Height="20" Width="200" FontSize="15" Margin="232,68,498,412"/>

        <Button x:Name="disSave" Click="disSave_Click" Content="Сохранить" Style="{StaticResource stdButton}" FontSize="15" HorizontalAlignment="Left" Margin="74,112,0,358" Width="135"/>
    </Grid>
</UserControl>
